{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": ["next/core-web-vitals", "prettier", "plugin:@typescript-eslint/recommended", "eslint:recommended", "@feature-sliced", "plugin:testing-library/react", "plugin:jest-dom/recommended"],
  "rules": {
    "no-console": "warn"
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  },
  "env": {
    "jest": true
  }
}
